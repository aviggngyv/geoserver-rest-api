{
    "name": "aviggngyv/geoserver-rest-api",
    "description": "A GeoServer API client",
    "type": "library",
    "homepage": "https://github.com/aviggngyv/geoserver-rest-api",
    "license": "MIT",
    "authors": [
        {
            "name": "aviggngyv",
            "email": "aviggngyv@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OneOffTech\\GeoServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },

        "files": [
            "tests/helpers.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "ext-fileinfo": "*",
        "ext-zip": "*",
        "jms/serializer": "^1.9|^3.4",
        "php-http/curl-client": "^2.2.0",
        "php-http/message": "^1.6",
        "php-http/discovery": "^1.3",
        "doctrine/annotations": "^1.4",
        "doctrine/cache": "^1.6",
        "php-http/client-common": "^1.5|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^8.5",
        "php-http/curl-client": "~1.7.0|^2.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "php-http/mock-client": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "symfony/var-dumper": "^4.1",
        "guzzlehttp/psr7": "^1.6",
        "http-interop/http-factory-guzzle": "^1.0",
        "mattkenefick/imagedifference": "dev-master"
    },
    "extra": {
        "branch-alias" : {
            "dev-master": "0.3.0-dev"
        }
    }
}
